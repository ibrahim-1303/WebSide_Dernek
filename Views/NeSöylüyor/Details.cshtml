@model Proje008.Models.NeSöylüyor

@{
    Layout = "_LayoutAdmin";
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Detay</title>
</head>
<body>

    <div>
        <h4>NeSöylüyor</h4>
        <hr />
        <table class="table table-bordered table-striped">
            <thead>


                <tr>
                    <th scope="col">#</th>
                    <th scope="col">#</th>
                </tr>

            </thead>
            <tbody>
                <tr>
                    <th scope="row">@Html.DisplayNameFor(model => model.ResimYolu)</th>
                    <td>
                        <div class="gallerycontainer">
                            <a class="thumbnail" href="#thumb"><img src="@("~/NeSöylüyorResimleri/"+Model.ResimYolu)" asp-append-version="true" width="100" height="66" border="0" /><span><img src="@("~/NeSöylüyorResimleri/"+Model.ResimYolu)" asp-append-version="true" /><br />@*@Model*@</span></a>
                        </div>
                    </td>
                </tr>
                <tr>
                    <th scope="row">@Html.DisplayNameFor(model => model.Aciklama)</th>
                    <td>@Html.DisplayFor(modelItem => Model.Aciklama)</td>
                </tr>
                <tr>
                    <th scope="row">@Html.DisplayNameFor(model => model.Ad)</th>
                    <td>@Html.DisplayFor(modelItem => Model.Ad)</td>
                </tr>
                <tr>
                    <th scope="row">@Html.DisplayNameFor(model => model.Meslek)</th>
                    <td>@Html.DisplayFor(modelItem => Model.Meslek)</td>
                </tr>
<td>

                <a asp-action="Edit" asp-route-id="@Model.Id">Değiştir</a> |
                <a asp-action="Index">Geri Dön</a>
            </td>

        </table>
</body>
</html>

