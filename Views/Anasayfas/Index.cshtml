@model IEnumerable<Proje008.Models.Anasayfa>

@{
    Layout = "_LayoutAdmin";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
   
</head>
<body>
<p>
    <a asp-action="Create">Ekle</a>
</p>
<table class="table table-bordered table-striped">

    @foreach (var item in Model)
    {
        <thead>


            <tr>
                <th scope="col">#</th>
                <th scope="col">#</th>
            </tr>

        </thead>
        <tbody>
            <tr>
                <th scope="row">Anasayfa Resimleri 1</th>
                <td>
                    <div class="gallerycontainer">
                        <a class="thumbnail" href="#thumb"><img src="@("~/AnasayfaResimleri/"+item.ResimYolu)" asp-append-version="true" width="100" height="66" border="0" /><span><img src="@("~/AnasayfaResimleri/"+item.ResimYolu)" asp-append-version="true" /><br />@*@Model*@</span></a>
                    </div>
                </td>
            </tr>


            <tr>
                <th>Anasayfa Resimleri 2</th>
                <td>
                    <div class="gallerycontainer">
                        <a class="thumbnail" href="#thumb"><img src="@("~/AnasayfaResimleri/"+item.ResimYolu1)" asp-append-version="true" width="100" height="66" border="0" /><span><img src="@("~/AnasayfaResimleri/"+item.ResimYolu1)" asp-append-version="true" /><br />@*@Model*@</span></a>
                    </div>
                </td>
            </tr>

            <tr>
                <th scope="row"> Anasayfa Resimleri 3</th>
                <td>
                    <div class="gallerycontainer">
                        <a class="thumbnail" href="#thumb"><img src="@("~/AnasayfaResimleri/"+item.ResimYolu2)" asp-append-version="true" width="100" height="66" border="0" /><span><img src="@("~/AnasayfaResimleri/"+item.ResimYolu2)" asp-append-version="true" /><br />@*@Model*@</span></a>
                    </div>
                </td>
            </tr>
            <tr>
                <th scope="row">@Html.DisplayNameFor(model => model.AnasayfaBaslik)</th>
                <td>@Html.DisplayFor(modelItem => item.AnasayfaBaslik)</td>
            </tr>
            <tr>
                <th scope="row">@Html.DisplayNameFor(model => model.AnasayfaAciklama)</th>
                <td>@Html.DisplayFor(modelItem => item.AnasayfaAciklama)</td>
            </tr>

            <tr>
                <th scope="row">@Html.DisplayNameFor(model => model.AnasayfaBaslik2)</th>
                <td>@Html.DisplayFor(modelItem => item.AnasayfaBaslik2)</td>
            </tr>
            <tr>
                <th scope="row">@Html.DisplayNameFor(model => model.AnasayfaAciklama2)</th>
                <td>@Html.DisplayFor(modelItem => item.AnasayfaAciklama2)</td>

            </tr>
            <tr>
                <th scope="row">Küçük Resim</th>
                <td>
                    <div class="gallerycontainer">
                        <a class="thumbnail" href="#thumb"><img src="@("~/AnasayfaResimleri/"+item.ResimYolu3)" asp-append-version="true" width="100" height="66" border="0" /><span><img src="@("~/AnasayfaResimleri/"+item.ResimYolu3)" asp-append-version="true" /><br />@*@Model*@</span></a>
                    </div>
                </td>

            </tr>
            <tr>
                <th scope="row">Küçük Başlık</th>
                <td>@Html.DisplayFor(modelItem => item.AnasayfaBaslik3)</td>

            </tr>
            <tr>
                <th scope="row">Küçük Açıklama</th>
                <td>@Html.DisplayFor(modelItem => item.AnasayfaAciklama3)</td>


            </tr>

            <tr>
                <th scope="row">Küçük Resim 2</th>
                <td>
                    <div class="gallerycontainer">
                        <a class="thumbnail" href="#thumb"><img src="@("~/AnasayfaResimleri/"+item.ResimYolu4)" asp-append-version="true" width="100" height="66" border="0" /><span><img src="@("~/AnasayfaResimleri/"+item.ResimYolu4)" asp-append-version="true" /><br />@*@Model*@</span></a>
                    </div>
                </td>

            </tr>
            <tr>
                <th scope="row">Küçük Başlık 2</th>
                <td>@Html.DisplayFor(modelItem => item.AnasayfaBaslik4)</td>
            </tr>
            <tr>
                <th scope="row">Küçük Açıklama 2</th>
                <td>@Html.DisplayFor(modelItem => item.AnasayfaAciklama4)</td>
            </tr>




<td>
            <a asp-action="Edit" asp-route-id="@item.Id">Düzenle</a> |
            <a asp-action="Details" asp-route-id="@item.Id">Detay</a> |
            <a asp-action="Delete" asp-route-id="@item.Id">Sil</a>
        </td>


        </tbody>
    }

</table>
</body>


</html>
