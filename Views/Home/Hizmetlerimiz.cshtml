@model IEnumerable<Proje008.Models.Hizmetlerimiz>



<body>



    <div class="site-preloader-wrap">
        <div class="spinner"></div>
    </div>

    <section class="page-header padding">
        <div class="container">
            <div class="page-content text-center">
                <h2>Specialized projects</h2>
                <p>Construction & Building Html Template.</p>
            </div>
        </div>
    </section>


    <section class="projects-section padding">
        <div class="container">
            <div class="row">

                @foreach (var item in Model)
                {
    <div class="col-lg-4 col-sm-6 padding-15">
        <div class="project-item">

            <img src="@("~/HizmetlerimizResimleri/" + item.ResimYolu)" alt="projects" asp-append-version="true" />
            <div class="overlay"></div>
            <a href="#" class="view-icon ajax-popup-link"> <i class="fas fa-expand"></i></a>
            <div class="projects-content">
                <a href="#" class="category">@item.Baslik</a>
                <h3><a href="#" class="tittle">@item.Aciklama</a></h3>
            </div>
        </div>
    </div>}


            </div>
        </div>
    </section>


</body>    