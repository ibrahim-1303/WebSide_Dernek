@model IEnumerable<Proje008.Models.Hakkimizda>

    
    
<body>
  
    <section class="page-header padding">
        <div class="container">
            <div class="page-content text-center">
                <h2>About company</h2>
                <p>Construction & Building Html Template.</p>
            </div>
        </div>
    </section>
  
   
    <section class="content-section padding">
        <div class="container">
            @foreach (var item in Model.TakeLast(1))
            {

            
            <div class="row content-wrap">
                <div class="col-lg-6 sm-padding">
                    <div class="content-info wow fadeInRight" data-wow-delay="300ms">
                        
                        <h2>@item.Baslik</h2>
                        <p>@item.Aciklama</p>
                       
                    </div>
                </div>
                <div class="col-lg-6 sm-padding wow fadeInLeft" data-wow-delay="100ms">
                    <img class="box-shadow" src="@("~/HakkimizdaResimleri/" +item.ResimYolu)"  alt="img" asp-append-version="true" >
                </div>
            </div>
            }
        </div>
    </section>
  
   
</body>
